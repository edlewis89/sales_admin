
<%= form_for(@user) do |f|%>
  <%= render 'shared/error_messages' %>
  <%= f.label  :email %>
  <%= f.text_field :email %>
  <%= f.label  :password %>
  <%= f.password_field :password %>
  <%= f.label  :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit "create"%>
<% end %>